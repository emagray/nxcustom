### NX CMM INSPECTION ###

# Last Modified: 8Jan2020

# This is the root for INSPECTION specific files
UGII_INSPECTION_BASE_DIR=${UGII_BASE_DIR}\CMM_INSPECTION\

# This is the root for platform independent resource files. Many other
# locations are defined based on this location.
UGII_INSPECTION_RESOURCE_DIR=${UGII_INSPECTION_BASE_DIR}resource\

# This is the NXcustom INSPECTION Resource folder
NXCUSTOM_UGII_INSPECTION_RESOURCE=${NXCUSTOM_LIB}\CMMresource\

UGII_INSPECTION_FILE_SEARCH_PATH=.\;${UGII_INSPECTION_BASE_DIR};${NXCUSTOM_UGII_INSPECTION_RESOURCE};${UGII_INSPECTION_RESOURCE_DIR}

# This is where default configuration .dat files are located. This is used to
# build the list of configurations to choose when entering Manufacturing.
# The custom directory is also searched for configurations.
#if FILE ${NXCUSTOM_UGII_INSPECTION_RESOURCE}configuration\
	UGII_INSPECTION_CONFIG_DIR=${NXCUSTOM_UGII_INSPECTION_RESOURCE}configuration\
#endif

# This is the location of template set .opt files
#if FILE ${NXCUSTOM_UGII_INSPECTION_RESOURCE}template_set\
	UGII_INSPECTION_TEMPLATE_SET_DIR=${NXCUSTOM_UGII_INSPECTION_RESOURCE}template_set\
#endif

# These are the locations of the english and metric template parts
#if FILE ${NXCUSTOM_UGII_INSPECTION_RESOURCE}template_part\
	UGII_INSPECTION_TEMPLATE_PART_DIR=${NXCUSTOM_UGII_INSPECTION_RESOURCE}template_part\
#endif

# If setup libraries or palettes are used, the dbc and ascii database are here.
#if FILE ${NXCUSTOM_UGII_INSPECTION_RESOURCE}template_dir\
	UGII_INSPECTION_LIBRARY_TEMPLATE_DIR=${NXCUSTOM_UGII_INSPECTION_RESOURCE}template_dir\
#endif

